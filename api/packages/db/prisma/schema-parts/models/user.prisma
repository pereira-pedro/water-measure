model User {
  id        String   @id @default(uuid()) @db.Uuid
  fullName  String   @map("full_name")
  email     String   @unique
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  addresses   Address[]
  authTokens  AuthToken[]
  waterMeters WaterMeter[]

  @@map("users")
}
